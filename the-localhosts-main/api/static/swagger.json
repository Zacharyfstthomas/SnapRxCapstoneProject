        "properties": {
          "userId": {
            "type": "integer",
            "required": true,
            "description": "ID of new user"
          },
          "token": {
            "type": "string",
            "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}",
            "required": true,
            "description": "Session token"
          },
          "message": {
            "type": "string",
            "required": false,
            "description": "Status message"
          },
          "errors": {
            "type": "array",
            "required": false,
            "description": "Array of errors"
          }
        }
      },
      "loginRequest": {
        "properties": {
          "email": {
            "type": "string",
